<%= form_for([@user, @user.posts.build]) do |f| %>

    <div>
      <%= f.label :Title %><br />
      <%= f.text_field :title%>
    </div>
    <div>
      <%= f.label :Content %><br />
      <%= f.text_area :content , :rows => 10, :cols => 30%>
    </div>
    <div>
      <%= f.submit %>
    </div>
<% end %>
