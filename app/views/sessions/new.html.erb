<h1> Sign In </h1>

<div>
  <div>
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :Email %>
      <%= f.text_field :email %>

      <%= f.label :Password %>
      <%= f.password_field :password %>

      <%= f.submit "Sign in" %>
    <% end %>

    <p>New user? <%= link_to "Sign up now!", signup_path %></p>
    <%= link_to "Home" , root_path %>

  </div>
</div>
